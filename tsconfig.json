{
  "compilerOptions": {
    "allowUnreachableCode": false,
    "strict": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": false,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "allowJs": true,
    "target": "ES2019",
    "outDir": "dist"
  },
  "include": ["**/*.ts"]
}
